{
    "manifest_version": 3,
    "name": "FullscreenEverything",
    "version": "0.0.0",
    "description": "Fullscreen everything in your browser's web page",
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_title": "Click to activate"
    },
    "background":
    {
        "service_worker": "main.js"
    },
    "icons": {
        "16": "/icons/16.png",
        "32": "/icons/32.png",
        "48": "/icons/48.png",
        "128": "/icons/128.png"
    }
}
